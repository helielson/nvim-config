{
  "snippets.autoTrigger": true,
  "tsserver.enableJavascript": true,
  "javascript.validate.enable": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "dart",
    "javascript"
  ],
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "disableWorkspaceFolders": true,
      "filetypes": ["go"]
    }
  }
}
